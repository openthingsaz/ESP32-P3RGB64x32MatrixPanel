          matrix.println();
          matrix.fillScreen(0);
          print_phone_num_gooood();
      }
      else if(cmd == 3)
      {
         print_arrow();
      }
    }    
    else 
    {
       // fill the screen with 'black'
      matrix.fillScreen(matrix.color444(0, 0, 0));
        
      matrix.setTextSize(1);     // size 1 == 8 pixels high
      matrix.setTextWrap(false); // Don't wrap at end of line - will do ourselves
      matrix.setCursor(0, 10);    // start at top left, with 8 pixel of spacing
      
      matrix.setTextColor(matrix.color444(0,0,7));
      for(int i=0; i<len; i++)
        matrix.print(temp[i]);
      delay(500);    
    }

  }
  
  
  if (Serial.available()) {
    //Serial.print(SerialBT.read());
    len = Serial.readBytes(temp, 20);
      
    //Serial.print("Input data Lenght : ");
    //Serial.println(len);  
    
    for(int i=0; i<len; i++)
      SerialBT.print(temp[i]);
      
    SerialBT.println();
  }
  

  
}

